<UserControl x:Class="SYAPSU.User_controls.listCommands"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SYAPSU.User_controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Border Background="White" BorderBrush="Black" BorderThickness="1.5 3 3 3">
            <DockPanel>
            <TextBlock DockPanel.Dock="Top" FontWeight="Bold" TextDecorations="Underline" Text="Упражнения на занятие" Background="#FFB1B1B1" FontSize="16" Foreground="Black" Margin="0,0,0,23" HorizontalAlignment="Center" VerticalAlignment="Center" IsEnabled="False" />
                <ListBox x:Name="listBoxCommands" AllowDrop="True" Drop="ListBoxCommandsDrop" FontSize="14" PreviewKeyDown="listBox_KeyDown" SelectionChanged="listBoxCommands_SelectionChanged">
                </ListBox>
            </DockPanel>
        </Border>
    </Grid>
</UserControl>
